{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "index": 0,
    "cases": [
      {
        "when": "tarnished",
        "model": {
          "type": "minecraft:select",
          "property": "minecraft:custom_model_data",
          "index": 1,
          "cases": [
            {
              "when": "open",
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:block/coffer/tarnished/open"
              }
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "fathoms:block/coffer/tarnished/closed"
          }
        }
      },
      {
        "when": "caliginous",
        "model": {
          "type": "minecraft:select",
          "property": "minecraft:custom_model_data",
          "index": 1,
          "cases": [
            {
              "when": "open",
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:block/coffer/caliginous/open"
              }
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "fathoms:block/coffer/caliginous/closed"
          }
        }
      }
    ]
  }
}