{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:context_dimension",
    "cases": [
      {
        "when": "minecraft:overworld",
        "model": {
          "type": "minecraft:range_dispatch",
          "property": "minecraft:time",
          "source": "moon_phase",
          "wobble": false,
          "scale": 8,
          "entries": [
            {
              "threshold": 0,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/0"
              }
            },
            {
              "threshold": 1,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/1"
              }
            },
            {
              "threshold": 2,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/2"
              }
            },
            {
              "threshold": 3,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/3"
              }
            },
            {
              "threshold": 4,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/4"
              }
            },
            {
              "threshold": 5,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/5"
              }
            },
            {
              "threshold": 6,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/6"
              }
            },
            {
              "threshold": 7,
              "model": {
                "type": "minecraft:model",
                "model": "fathoms:item/ornate_moondial/7"
              }
            }
          ]
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "fathoms:item/ornate_moondial/wrong_dimension"
    }
  }
}