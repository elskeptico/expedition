{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:display_context",
    "cases": [
      {
        "when": "head",
        "model": {
          "type": "minecraft:condition",
          "property": "minecraft:custom_model_data",
          "index": 0,
          "on_true": {
            "type": "minecraft:model",
            "model": "fathoms:entity/historian/hurt/head"
          },
          "on_false": {
            "type": "minecraft:model",
            "model": "fathoms:entity/historian/default/head"
          }
        }
      }
    ],
    "fallback": {
      "type": "minecraft:select",
      "property": "minecraft:context_entity_type",
      "cases": [
        {
          "when": "minecraft:zombie_villager",
          "model": {
            "type": "minecraft:empty"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:condition",
        "property": "minecraft:custom_model_data",
        "index": 0,
        "on_true": {
          "type": "minecraft:composite",
          "models": [
            {
              "type": "minecraft:model",
              "model": "fathoms:entity/historian/hurt/body"
            },
            {
              "type": "minecraft:model",
              "model": "fathoms:entity/historian/hurt/arms"
            }
          ]
        },
        "on_false": {
          "type": "minecraft:composite",
          "models": [
            {
              "type": "minecraft:model",
              "model": "fathoms:entity/historian/default/body"
            },
            {
              "type": "minecraft:model",
              "model": "fathoms:entity/historian/default/arms"
            }
          ]
        }
      }
    }
  }
}